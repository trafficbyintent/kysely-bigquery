{
  "name": "@traffic.by.intent/kysely-bigquery",
  "version": "1.4.0",
  "description": "BigQuery Dialect for Kysely",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/trafficbyintent/kysely-bigquery"
  },
  "scripts": {
    "postinstall": "node scripts/generate-tsconfig.js",
    "build": "tsc",
    "test": "vitest run --exclude=\"**/*.integration.test.ts\"",
    "test:all": "vitest run",
    "test:integration": "vitest run **/*.integration.test.ts",
    "test:watch": "vitest --exclude='**/*.integration.test.ts'",
    "test:github-actions": "./.github/test-actions.sh all",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "test:coverage": "vitest run --coverage --exclude=\"**/*.integration.test.ts\"",
    "test:coverage:all": "vitest run --coverage"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "kysely",
    "bigquery",
    "bq"
  ],
  "author": "Traffic by Intent",
  "contributors": [
    "Makara Sok",
    "Bradford Melluish"
  ],
  "license": "MIT",
  "devDependencies": {
    "@google-cloud/bigquery": "^8.1.1",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv": "^17.2.0",
    "eslint": "^8.56.0",
    "kysely": "^0.28.4",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "@trafficbyintent/style-guide": "^1.1.10"
  },
  "peerDependencies": {
    "@google-cloud/bigquery": "*",
    "kysely": "*"
  }
}
